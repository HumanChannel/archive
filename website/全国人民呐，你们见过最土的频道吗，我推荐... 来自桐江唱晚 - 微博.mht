@charset "utf-8";

.S_spetxt, a.S_ficon:hover, a:hover .S_ficon, a.current .S_ficon { color: rgb(250, 125, 60); }

.S_error { color: rgb(255, 0, 0); }

.S_spetxt_bg { background-color: rgb(250, 125, 60); }

.W_btn_prev:hover, .W_btn_next:hover { border-color: rgb(250, 125, 60); }

.W_btn_prev:hover, .W_btn_next:hover, a.S_ficon:hover, a:hover .S_ficon, a.current .S_ficon, a.S_txt1:hover, a.current .S_txt1, a.S_txt2:hover, .SW_fun:hover .S_func1 { text-decoration: none; cursor: pointer; }

.S_ficon_dis, a.S_ficon_dis:hover, a:hover .S_ficon_dis { cursor: default; opacity: 0.5; }

.WB_timeline .current em { color: rgb(255, 255, 255); }

.send_weibo { background-image: none; }

.WB_miniblog { padding-top: 50px; }

.S_page .WB_miniblog { padding-top: 50px; }

.S_page .WB_frame { background-color: transparent !important; }

#js_skin_public_base { height: 42px; }

body, .S_page { background-color: rgb(180, 218, 240); background-image: url("images/body_repeat.png?id=1414052827385"); background-repeat: repeat-x; background-position: center 0px; }

.send_weibo .ficon_swtxt { color: rgb(27, 127, 182); }

body, legend, .W_input:focus, .S_txt1, .W_btn_b, .SW_fun .S_func1 { color: rgb(51, 51, 51); text-decoration: none; }

.S_txt1_bg { background-color: rgb(51, 51, 51); }

.S_txt1_br { border-color: rgb(51, 51, 51); }

.S_txt2, .W_input, .W_btn_b_disable, .W_btn_b_disable:hover { color: rgb(128, 128, 128); text-decoration: none; }

.S_txt2_bg { background-color: rgb(128, 128, 128); }

.S_txt2_br { border-color: rgb(128, 128, 128); }

.S_ficon, .S_ficon_dis, a.S_ficon_dis:hover, a:hover .S_ficon_dis { color: rgb(105, 110, 120); }

.S_ficon_bg { background-color: rgb(105, 110, 120); }

.S_ficon_br { border-color: rgb(105, 110, 120); }

a, .S_link1, a.S_txt1:hover, a.current .S_txt1, a.S_txt2:hover, .SW_fun:hover .S_func1 { color: rgb(235, 115, 80); }

.S_link1_bg { background-color: rgb(235, 115, 80); }

.S_link1_br { border-color: rgb(235, 115, 80); }

.S_bg1, .SW_fun_bg:hover, .SW_fun_bg_active { background-color: rgb(242, 242, 245); }

.S_bg1_c { color: rgb(242, 242, 245); }

.S_bg1_br { border-color: rgb(242, 242, 245); }

.W_btn_cardlink, .W_btn_tag_cur, .W_btn_tag_cur:hover { background-color: rgb(242, 242, 245) !important; }

.S_bg2, blockquote, .W_btn_b, .W_input, .SW_fun_bg { background-color: rgb(255, 255, 255); }

.S_bg2_c { color: rgb(255, 255, 255); }

.S_bg2_br { color: rgb(255, 255, 255); border-color: rgb(255, 255, 255); }

.S_bg3 { background-color: rgb(234, 234, 236); }

.S_line1, .W_btn_prev, .W_btn_next, .W_btn_b { border-color: rgb(217, 217, 217); }

.W_btn_b_disable, .W_btn_b_disable:hover, .W_btn_tag_cur, .W_btn_tag_cur:hover { border-color: rgb(217, 217, 217) !important; }

.S_line1_c { color: rgb(217, 217, 217); }

.W_btn_b_disable .S_ficon { color: rgb(217, 217, 217) !important; }

.S_line2 { border-color: rgb(242, 242, 245); }

.S_line2_c { color: rgb(242, 242, 245); }

.S_line3, .W_input, .send_weibo .input, .W_btn_b:hover { border-color: rgb(204, 204, 204); }

.W_input, .send_weibo .input { background-color: rgb(255, 255, 255); }

.WB_left_nav .S_txt1, .WB_left_nav a.S_txt1:hover, .WB_left_nav .S_ficon, .WB_left_nav a:hover .S_ficon, .WB_left_nav a.S_ficon:hover { color: rgb(255, 255, 255); }

.WB_left_nav .lev a:hover, .WB_left_nav .lev_curr, .WB_left_nav .lev_curr:hover, .WB_left_nav .levmore .more { background-color: rgba(255, 255, 255, 0.2); }

.WB_left_nav .lev_Box, .WB_left_nav fieldset { border-color: rgba(255, 255, 255, 0.2); }

.WB_frame { background-color: rgba(70, 149, 194, 0.3); }

.WB_timeline .S { color: rgb(71, 166, 217); border-left-color: rgb(71, 166, 217); }

.WB_timeline .current em, .WB_timeline .S_dot, .W_gotop { background-color: rgb(71, 166, 217); }

.WB_miniblog { background: url("images/body_bg.jpg?id=201503261330") center top no-repeat; }

.S_page .WB_miniblog { background: url("images/body_bg_page.jpg?id=201503261330") center top no-repeat; }

#js_skin_skin048_skin { height: 42px; }
