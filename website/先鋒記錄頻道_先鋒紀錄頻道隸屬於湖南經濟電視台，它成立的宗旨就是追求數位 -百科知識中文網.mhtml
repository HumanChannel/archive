@charset "utf-8";

.tocify a, .tocify li:hover a { font-family: "Microsoft JhengHei", "Microso=
ft YaHei", Verdana, sans-serif; color: rgb(0, 0, 0); }

.post-nav.fixed { position: fixed; top: 0px; left: 80%; }

.post-nav .nav-heading { border-top: none; border-right: none; border-left:=
 none; border-image: initial; padding: 16px 12px 4px 15px; font-size: 15px;=
 font-weight: bold; border-bottom: 1px solid rgb(221, 221, 221); display: b=
lock; }

.tocify { width: 100%; max-height: 90%; overflow: auto; font-size: 15px; }

.tocify li { border-left: 3px solid rgb(243, 243, 243); transition: all 0.2=
s ease 0s; }

.tocify li:hover { border-left: 3px solid rgb(0, 0, 0); }

.tocify a { display: block; }

.tocify .active a, .tocify a:hover { color: rgb(48, 52, 233); }

.tocify a:active, .tocify a:focus { background-color: transparent; }

.tocify .active { border-left: 3px solid rgb(48, 52, 233); background-color=
: rgb(243, 243, 243); }

.tocify li, .tocify ul { margin: 0px; padding: 0px; line-height: 25px; }

.tocify-header { text-indent: 10px; white-space: nowrap; overflow: hidden; =
text-overflow: ellipsis; }

.tocify-subheader { text-indent: 26px; display: none; white-space: nowrap; =
overflow: hidden; text-overflow: ellipsis; }

.tocify-subheader li { font-size: 13px; }

.tocify-subheader .tocify-subheader { text-indent: 36px; }

.tocify-subheader .tocify-subheader .tocify-subheader { text-indent: 54px; =
}

.nav-list .nav-header, .nav-list > li > a { margin: 0px; }

.nav-list > li > a { padding: 1px 5px; }
